{
  "name": "exclamate-gql",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "apollo-server": "^2.9.0",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "log4js": "^4.5.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-resolvers": "1.6.1",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.137",
    "@types/node": "^8.10.52",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "3.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  }
}
